{
  "name": "rest-api-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "~14"
  },
  "scripts": {
    "start-backend": "nodemon index.js",
    "start-api": "json-server --host 127.0.0.1 ./api.js",
    "delay-api": "json-server  --host 127.0.0.1 ./api.js --delay 10000",
    "start-front": "npm start --prefix front",
    "test-back": "jest --verbose --rootDir=./test",
    "test-front": "npm run test --prefix front",
    "dev": "concurrently \"nodemon index.js\" \"npm run start-front\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julien-Decoen/rest-api-test.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Julien-Decoen/rest-api-test/issues"
  },
  "homepage": "https://github.com/Julien-Decoen/rest-api-test#readme",
  "dependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.3",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "concurrently": "^7.2.2",
    "css-loader": "^6.7.1",
    "express": "^4.18.1",
    "express-react-views": "^0.11.0",
    "json-server": "^0.17.0",
    "mini-css-extract-plugin": "^2.6.0",
    "moxios": "^0.4.0",
    "node-cache": "^5.1.2",
    "node-sass": "^7.0.1",
    "nunjucks": "^3.1.3",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "sass-loader": "^12.6.0",
    "save-dev": "^0.0.1-security",
    "style-loader": "^0.23.1",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0",
    "webpack": "^5.70.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "babel-preset-env": "^1.7.0",
    "jest": "^28.1.2",
    "nodemon": "^2.0.18",
    "supertest": "^6.2.4",
    "webpack-cli": "^4.9.2"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git/*",
      "node_modules/*",
      ".public/*"
    ],
    "verbose": true,
    "execMap": {
      "js": "node index.js"
    },
    "events": {
      "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
    },
    "watch": [
      "app/*",
      "front/*"
    ],
    "jest": {
      "testEnvironment": "node",
      "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$",
      "moduleFileExtensions": ["js", "json", "jsx"],
      "coveragePathIgnorePatterns": [
        "/node_modules/"
      ]
    },
    "ext": "js json html css"
  }
}
